{
	"extends": [
		"eslint:recommended",
		"plugin:react/recommended"
	],
	"plugins": ["jest"],
	"env": {
		"node": true,
		"browser": true,
		"jest/globals": true
	},
	"rules": {
		"react/react-in-jsx-scope": "off",
		"indent": ["error", "tab"],
		"no-unused-vars": "warn",
		"quotes": "error"
	},
	"parser": "babel-eslint"
}